{
  "name": "@ideditor/location-conflation",
  "version": "0.1.0",
  "license": "ISC",
  "repository": "ideditor/location-conflation",
  "author": "Bryan Housel <bhousel@gmail.com>",
  "description": "Define complex geographic regions by including and excluding country codes and geojson shapes",
  "keywords": [
    "location",
    "include",
    "exclude",
    "geojson"
  ],
  "main": "dist/index.js",
  "module": "index.mjs",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint index.mjs test/*.js",
    "test": "npm run build && npm run lint && tap --reporter terse --no-coverage test/*.js"
  },
  "dependencies": {
    "@ideditor/country-coder": "^3.0.1",
    "@mapbox/geojson-area": "^0.2.2",
    "@turf/difference": "^5.1.5",
    "@turf/union": "^5.1.5",
    "circle-to-polygon": "^1.0.2",
    "geojson-precision": "^0.4.0",
    "geojson-rewind": "^0.3.1",
    "json-stringify-pretty-compact": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^6.0.0",
    "rollup": "^1.27.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "tap": "^14.10.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
