{
  "name": "@ideditor/location-conflation",
  "version": "0.3.0",
  "license": "ISC",
  "repository": "ideditor/location-conflation",
  "author": "Bryan Housel <bhousel@gmail.com>",
  "description": "Define complex geographic regions by including and excluding country codes and geojson shapes",
  "keywords": [
    "location",
    "locationset",
    "include",
    "exclude",
    "geojson"
  ],
  "main": "dist/index.js",
  "module": "index.mjs",
  "scripts": {
    "all": "npm run build",
    "build": "npm-run-all -s dist:**",
    "dist:js": "rollup --config config/rollup.config.js",
    "dist:es5": "rollup --config config/rollup.config.es5.js",
    "lint": "eslint index.mjs test/*.js",
    "test": "npm run build && npm run lint && tap --reporter terse --no-coverage test/*.js"
  },
  "dependencies": {
    "@aitodotai/json-stringify-pretty-compact": "^1.3.0",
    "@ideditor/country-coder": "^3.1.0",
    "@mapbox/geojson-area": "^0.2.2",
    "@turf/difference": "^5.1.5",
    "@turf/union": "^5.1.5",
    "circle-to-polygon": "^2.0.1",
    "geojson-precision": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "eslint": "^6.0.0",
    "npm-run-all": "^4.0.0",
    "rollup": "^2.14.0",
    "tap": "^14.10.0"
  },
  "engines": {
    "node": ">=10"
  }
}
